{
  "name": "@pydantic/genai-prices",
  "version": "0.0.21",
  "description": "Calculate prices for calling LLM inference APIs",
  "author": "Pydantic Team",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/pydantic/genai-prices/",
  "bugs": {
    "url": "https://github.com/pydantic/genai-prices/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "pydantic",
    "genai",
    "prices",
    "llm",
    "inference",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pydantic/genai-prices"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "genai-prices": "dist/cli.js"
  },
  "scripts": {
    "build": "vite build && npm run build:cli",
    "build:cli": "vite build --config vite.config-cli.ts",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format": "eslint --fix --quiet",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "browser-example": "vite --config src/examples/browser/vite.browser-preview.ts",
    "node-example": "tsx ./src/examples/node-script.ts",
    "prepare": "npm run build",
    "knip": "knip"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.17.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/utils": "^8.26.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-turbo": "^2.4.4",
    "globals": "^16.0.0",
    "knip": "^5.62.0",
    "neostandard": "^0.12.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^7.1.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT"
}
